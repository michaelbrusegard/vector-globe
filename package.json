{
  "name": "vector-globe",
  "version": "0.1.0",
  "author": "Michael Brusegard",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --splitting --experimental-dts --format cjs,esm",
    "dev": "bun run --hot server.ts",
    "watch": "tsup src/index.ts --watch --sourcemap --format esm",
    "lint": "eslint . --ext .ts,.tsx,.cjs,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelbrusegard/Vector-Globe.git"
  },
  "bugs": {
    "url": "https://github.com/michaelbrusegard/Vector-Globe/issues"
  },
  "homepage": "https://michaelbrusegard.github.io/Vector-Globe/",
  "devDependencies": {
    "@babel/standalone": "^7.23.10",
    "@microsoft/api-extractor": "^7.40.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint": "^8.56.2",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "hono": "^4.0.5",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^23.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "three": "^0.160.1"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "packageManager": "bun@1.0.25"
}
